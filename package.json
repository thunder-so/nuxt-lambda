    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "aws-cdk": "2.139.0",
    "aws-cdk-lib": "2.139.0",
    "constructs": "^10.3.0",
    "shelljs": "^0.8.5",
    "submodules-install": "^1.1.0"
